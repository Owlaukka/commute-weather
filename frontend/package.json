{
  "name": "commute-weather-frontend",
  "version": "0.0.1",
  "description": "Frontend for the Commute Weather web app.",
  "private": true,
  "author": "Olli Laukkanen",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack/webpack.dev.js",
    "start:test": "cross-env NODE_ENV=test webpack-dev-server --config webpack/webpack.dev.js",
    "test": "cross-env NODE_ENV=test cypress open",
    "test-e2e": "cd ../api && start-server-and-test ci 3000 'cd ../frontend && start-server-and-test start:test 8080 test'",
    "build": "webpack --config webpack/webpack.prod.js",
    "ci": "yarn run lint && cross-env CYPRESS_BASE_URL=http://localhost:3000 cypress run",
    "lint": "yarn run check-types && eslint src/ cypress/ --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives",
    "analyze": "yarn run analyze:build && yarn run analyze:show",
    "analyze:build": "webpack --profile --progress --env production --config webpack/webpack.prod.js --json > stats.json",
    "analyze:show": "webpack-bundle-analyzer stats.json dist/",
    "check-types": "tsc"
  },
  "dependencies": {
    "@apollo/client": "^3.2.4",
    "@apollo/react-hooks": "^4.0.0",
    "@babel/runtime": "^7.11.2",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "apollo-cache-persist": "^0.1.1",
    "core-js": "^3.6.5",
    "css-in-js-media": "^1.0.15",
    "dayjs": "^1.9.3",
    "emotion-theming": "^10.0.27",
    "graphql": "^15.3.0",
    "normalize.css": "^8.0.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@cypress/webpack-preprocessor": "^5.4.8",
    "@testing-library/cypress": "^7.0.1",
    "@types/bluebird": "^3.5.32",
    "@types/cypress": "^1.1.3",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "apollo-cache": "^1.3.5",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^6.0.3",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cypress": "^5.4.0",
    "cypress-react-unit-test": "^4.16.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-typescript": "^11.0.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "execa": "^4.0.3",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.3.0",
    "prettier": "^2.1.1",
    "start-server-and-test": "^1.11.3",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-prettier": "^1.1.2",
    "terser-webpack-plugin": "^4.2.2",
    "typescript": "^4.0.3",
    "webpack": "^5.1.3",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  }
}
